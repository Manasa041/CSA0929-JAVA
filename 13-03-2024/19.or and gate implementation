public class Main {
 static boolean ORGate(boolean input1, boolean input2) {
        return input1 || input2; // Uses logical OR operator
    }
    public static boolean ANDGate(boolean input1, boolean input2) {
        return input1 && input2; // Uses logical AND operator
    }
    public static void main(String[] args) {
        boolean a = true;
        boolean b = false;

        System.out.println("OR Gate:");
        System.out.println("a OR b = " + ORGate(a, b)); // Output: true (a is true)

        System.out.println("\nAND Gate:");
        System.out.println("a AND b = " + ANDGate(a, b)); // Output: false (both need to be true)
    }
}
